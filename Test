elastic.apm.enabled=true
elastic.apm.config.service_name=my-spring-boot-app
elastic.apm.config.server_url=http://localhost:8200
elastic.apm.config.secret_token=
elastic.apm.config.application_packages=com.example
elastic.apm.config.trace_methods=com.example.service.*,com.example.controller.*
elastic.apm.config.enable_experimental_instrumentations=true
elastic.apm.config.capture_jdbc_statements=true
elastic.apm.config.transaction_sample_rate=0.5


@Value("${elastic.apm.enabled:false}")
    private boolean apmEnabled;

    private final Map<String, String> config = new HashMap<>();

    public Map<String, String> getConfig() {
        return config;
    }

@ConfigurationProperties(prefix = "elastic.apm.config")
